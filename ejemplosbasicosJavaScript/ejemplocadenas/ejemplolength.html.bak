<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <title>ejemplosubstring</title>
    <script type="text/javascript">
// Ejemplo cadenas transparancia66
var mensaje = "Hola Mundo";
var porcion = mensaje.substring(2); // porcion = "la Mundo"
alert(porcion);

porcion = mensaje.substring(5); // porcion = "Mundo"
alert(porcion);      

porcion = mensaje.substring(7); // porcion = "ndo"
alert(porcion); 
      
      
// Ejemplo cadenas transparancia67      
var mensaje = "Hola Mundo";
var porcion = mensaje.substring(1, 8); // porcion = "ola Mun"
alert(porcion);
porcion = mensaje.substring(3, 4); // porcion = "a"
alert(porcion);
      
// USAR LA FUNCION SUBSTRING PARA QUEDAROS CON 
// le ponemos final
porcion = mensaje.substring(5,10);      
alert(porcion);
      
      
      

        
        
  </script>
  </head>
  <body> 
    <p><br>
    </p>
  </body>
</html>
